html
  head
    title= 'GAPI TEST'
    style
      include style.css
  body
    h1= 'GAPI Table Generated below...'
    table.table-style
      each row, i in rows
        if i === 0
          thead
            tr
              each cell in row
                th= cell
        if i > 0
          tbody
            tr
              each cell in row
                td= cell